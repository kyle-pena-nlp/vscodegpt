{
  "name": "vscode-gpt",
  "displayName": "vscode-gpt",
  "description": "A project-aware, goal-based way to write code with ChatGPT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-gpt.suggestGoals",
        "title": "Suggest Goals"
      },
      {
        "command": "vscode-gpt.suggestProjectChanges",
        "title": "Suggest Project Changes"
      },
      {
        "command": "vscode-gpt.executeUserSpecifiedProjectChangesCommand",
        "title": "Modify My Project"
      },
      {
        "command": "vscode-gpt.executeUserSpecifiedCodeChangesCommand",
        "title": "Modify My Code"
      }            
    ],
    "configuration": {
      "type": "object",
      "title": "vscode-gpt apiKey",
      "properties": {
        "vscode-gpt.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your API key for ChatGPT (See: https://platform.openai.com/account/api-keys)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "directory-tree": "^3.5.1",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "openai": "^3.2.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "openai": "^3.2.1"
  }
}
